Store;\r\n\r\n  useEffect(() => {\r\n    for (let i = 0; i < brands.length; i++) {\r\n      if (brands[i].id === device.brandId) {\r\n        setName(brands[i].name);\r\n        break;\r\n      }\r\n    }\r\n    fetchDeviceRatings(device.id).then(({ count, rows }) => {\r\n      const amount = rows.reduce((agg, item) => {\r\n        return (agg += item.rate);\r\n      }, 0);\r\n      setCount(count);\r\n      setRate(calculationRate(amount, count));\r\n    });\r\n  }, []);\r\n  const history = useHistory();\r\n  return (\r\n    <Col\r\n      md={3}\r\n      className={\"mt-3\"}\r\n      onClick={() => history.push(DEVICE_ROUTE + \"/\" + device.id)}\r\n    >\r\n      <Card style={{ width: 150, cursor: \"pointer\" }} border=\"light\">\r\n        <Image\r\n          width={150}\r\n          height={150}\r\n          src={process.env.REACT_APP_API_URL + device.img}\r\n        />\r\n        <div className=\"text-black-50 d-flex justify-content-between align-items-center mt-2\">\r\n          <div>{name}</div>\r\n          <div className=\"d-flex align-items-center\">\r\n            <div>\r\n              {rate}/{count}\r\n            </div>\r\n            <Image width={18} height={18} src={star} />\r\n          </div>\r\n        </div>\r\n        <div>{device.name}</div>\r\n      </Card>\r\n    </Col>\r\n  );\r\n});\r\n","C:\\Users\\looka\\Desktop\\internet-shop_ulbi\\client\\src\\components\\modals\\CreateDevice.js",["129"],"import { observer } from \"mobx-react-lite\";\r\nimport { useContext, useEffect, useState } from \"react\";\r\nimport { Modal, Button, Form, Dropdown, Row, Col } from \"react-bootstrap\";\r\nimport { fetchTypes, fetchBrands, createDevice } from \"../../http/deviceApi\";\r\nimport { Context } from \"../../index\";\r\nexport const CreateDevice = observer(({ show, onHide }) => {\r\n  const { device } = useContext(Context);\r\n  const [name, setName] = useState(\"\");\r\n  const [price, setPrice] = useState(0);\r\n  const [file, setFile] = useState(null);